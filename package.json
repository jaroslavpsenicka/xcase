{
  "name": "xcase",
  "version": "0.1.0",
  "description": "xCase",
  "main": "server/server.js",
  "scripts": {
    "server": "nodemon server/server.js",
    "client": "cross-env NODE_ENV=development REACT_APP_VERSION=$npm_package_version REACT_APP_SERVICE_URL=http://localhost:8080 webpack-dev-server --hot --history-api-fallback",
    "build": "cross-env NODE_ENV=production REACT_APP_VERSION=$npm_package_version webpack",
    "build:xhypo-overview": "cd lib/xhypo-overview && npm run build:elements && cp elements/xhypo-overview.js* ../../client/static",
    "build:xhypo-create": "cd lib/xhypo-create && yarn build && cp dist/xhypo-create.js* ../../client/static",
    "build:xhypo-detail": "cd lib/xhypo-detail && yarn wc-build && cp dist/xhypo-detail.js* ../../client/static",
    "build:addon-chat": "cd lib/addon-chat && yarn build && cp dist/addon-chat.js* ../../client/static",
    "test": "cd server && mocha",
    "start": "node server/server.js"
  },
  "author": "Jaroslav Psenicka",
  "license": "ISC",
